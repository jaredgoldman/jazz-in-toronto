{
    "name": "jazz-in-toronto",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev && pnpm run codegen",
        "postinstall": "prisma generate",
        "lint": "next lint",
        "start": "next start",
        "test": "vitest",
        "studio": "prisma studio",
        "db:push": "prisma db push",
        "db:reset": "NODE_ENV=development prisma migrate reset --force --skip-seed && pnpm run db:push && pnpm run db:seed",
        "db:clear": "NODE_ENV=development prisma migrate reset --force --skip-seed",
        "db:seed": "NODE_ENV=development prisma db seed",
        "schema:format": "prisma format",
        "prepare": "husky install",
        "codegen": "graphql-codegen --config codegen.ts"
    },
    "prisma": {
        "seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@graphql-typed-document-node/core": "^3.2.0",
        "@next-auth/prisma-adapter": "^1.0.5",
        "@prisma/client": "^4.16.2",
        "@t3-oss/env-nextjs": "^0.3.1",
        "@tanstack/react-query": "^4.29.7",
        "@trpc/client": "^10.26.0",
        "@trpc/next": "^10.26.0",
        "@trpc/react-query": "^10.26.0",
        "@trpc/server": "^10.26.0",
        "@uploadthing/react": "^5.2.0",
        "cheerio-json-mapper": "^1.0.3",
        "date-fns": "^2.30.0",
        "formik": "^2.4.2",
        "graphql": "^16.7.1",
        "graphql-request": "^6.1.0",
        "next": "^13.4.2",
        "next-auth": "^4.22.1",
        "nodemailer": "^6.9.4",
        "puppeteer": "^21.0.1",
        "react": "18.2.0",
        "react-cool-onclickoutside": "^1.7.0",
        "react-datepicker": "^4.15.0",
        "react-dom": "18.2.0",
        "react-dropzone": "^14.2.3",
        "superjson": "1.12.2",
        "uploadthing": "^5.2.1",
        "use-places-autocomplete": "^4.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/client-preset": "4.1.0",
        "@types/eslint": "^8.37.0",
        "@types/node": "^18.16.0",
        "@types/prettier": "^2.7.2",
        "@types/react": "^18.2.6",
        "@types/react-datepicker": "^4.11.2",
        "@types/react-dom": "^18.2.4",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.59.6",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.40.0",
        "eslint-config-next": "^13.4.2",
        "husky": "^8.0.0",
        "lint-staged": "^13.2.3",
        "postcss": "^8.4.21",
        "prettier": "^2.8.8",
        "prettier-plugin-tailwindcss": "^0.2.8",
        "prisma": "^4.16.2",
        "tailwindcss": "^3.3.0",
        "ts-node": "^10.9.1",
        "tsx": "^3.12.7",
        "typescript": "^5.0.4",
        "vitest": "^0.32.2"
    },
    "ct3aMetadata": {
        "initVersion": "7.14.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pnpm run lint"
        }
    }
